%echo-on
%no-protection
Key-Type: {{ gpg_key_type }}
Key-Length: {{ gpg_key_length }}
Subkey-Type: {{ gpg_key_type }}
Subkey-Length: {{ gpg_key_length }}
Name-Real: {{ gpg_real_name }}
Name-Email: {{ gpg_email }}
Expire-Date: {{ gpg_expire_date }}
{% if gpg_passphrase %}
Passphrase: {{ gpg_passphrase }}
{% endif %}
%commit
%echo done
